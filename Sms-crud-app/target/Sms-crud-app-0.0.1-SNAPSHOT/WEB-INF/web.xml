<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>StudentSaveController</servlet-name>
  	<display-name>StudentSaveController</display-name>
  	<description></description>
  	<servlet-class>com.ciq.sms.controller.StudentSaveController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentFindAllController</servlet-name>
  	<display-name>StudentFindAllController</display-name>
  	<description></description>
  	<servlet-class>com.ciq.sms.controller.StudentFindAllController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StudentSaveController</servlet-name>
  	<url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentFindAllController</servlet-name>
  	<url-pattern>/sms-list</url-pattern>
  </servlet-mapping>
</web-app>
